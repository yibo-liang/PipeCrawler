<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="pcc.core.entity.RawUser" table="users">
    <id name="sequence_id">
      <generator class="assigned"/>
    </id>
    <property name="wb_id"/>
    <property name="container_id"/>
    <property name="screen_name"/>
    <property name="verified"/>
    <property name="v_reason"/>
    <property name="v_type"/>
    <property name="is_member"/>
    <property name="gender"/>
    <property name="statuses_count"/>
    <property name="description"/>
    <property name="description_length"/>
    <property name="fans_num"/>
    <property name="follow_num"/>
    <property name="last_update"/>
    <property name="last_check"/>
    <property name="crawling_state"/>
  </class>
</hibernate-mapping>
